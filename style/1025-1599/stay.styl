//-------------------------------------------- structure

// stay

#stay .choose
    clearfloat()
    section div
        float: left
        width: _8
        margin-top: 60px
        &:first-of-type
            & a
                color: #E8E3DC
                background: _brown url('../images/stay/cottages.jpg') no-repeat center
                background-size: cover
                border-color: _brown
                &:hover
                    opacity: 0.9
                    border-color: darken(_brown, 40%)
        &:last-of-type
            & a
                color: #E8EDE9
                background: _green url('../images/stay/campsite.jpg') no-repeat center
                background-size: cover
                border-color: _green
                &:hover
                    opacity: 0.9
                    border-color: darken(_green, 40%)
        & img
            float: left
            margin: 8px 18px 0 0
        & p
            font-size: 1.4em
            margin-top: 0
            margin-bottom: 1.2em
        & a
            display: block
            width: 100%
            padding: 26% 0 22% 0
            border-radius: 12px
            border: none
            font-family: heavyweightregular, sans_serif
            text-transform: lowercase
            font-size: 5.6em
            text-align: center
            border-width: 6px
            border-style: solid
            text-shadow: 1px 1px 0 #000
            text-decoration: none
            line-height: 0
            transition(all 250ms ease)

// stay → cottages + campsite

#stay .cottages, #stay .campsite
    margin-top: 70px
    & section:first-of-type
        padding: 2.8em 0 1.4em 0
        background-size: contain
        text-shadow: 1px 1px 0 #F8F2EE
        & h2
            font-size: 5.8em
            text-align: center
            margin: 0 0 0.13em 0
        & h3
            font-size: 2.4em
            text-align: center
            margin: 0
    & section:last-of-type
        margin: 45px 0 40px 0
        clearfloat()
        & a
            position: relative
            float: left
            width: _4
            margin: auto
            padding: 12.6% 0
            text-decoration: none
            background-size: cover !important
            & span
                position: absolute
                top: 0
                left: 0
                width: _16
                background-size: cover !important
                margin: 0
                padding: 50.4% 0
                text-shadow: 1px 1px 0 _light_brown
                transition(opacity 400ms ease)
                & h4
                    font-size: 5em
                    text-align: center
                    line-height: 0
                    margin: 0
                & p
                    position: absolute
                    bottom: 12%
                    left: 50%
                    width: _16
                    transform(translate(-50%, 0%))
                    font-size: 1.1em
                    text-align: center
                    line-height: 0
                    margin: 0
                &:hover
                    opacity: 0

