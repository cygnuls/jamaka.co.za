// stay → cottages → units +  stay → campsite → units

#stay .cottages.units
#stay .campsite.units
    margin-top: 70px
    & > div:nth-of-type(1)
        width: _16
        margin: 0 0 -0.2em 0
        padding: 0
        clearfloat()
        & h2
            color: _light_brown
            font-size: 4.3em
            letter-spacing: 8px
            line-height: 1.7
            text-align: center
            text-shadow: -1px -1px 0 #2B1B03
            padding-top: 0.2em
            background: transparent url('../images/stay/cottages/header_units.png') no-repeat center
    & > div:nth-of-type(2)
        margin: 0
        clearfloat()
        & img
            display: block
            margin: 0 auto
        & #display
            float: left
            width: _11
            position: relative
            margin: 0
            padding: 0
            padding-bottom: 32.5%
            & img
                position: absolute
                width: _16
                top: 0
                left: 0
        & #gallery
            float: left
            width: _5
            margin: 0
            padding: 0
            & a
                float: left
                width: 33.3%
